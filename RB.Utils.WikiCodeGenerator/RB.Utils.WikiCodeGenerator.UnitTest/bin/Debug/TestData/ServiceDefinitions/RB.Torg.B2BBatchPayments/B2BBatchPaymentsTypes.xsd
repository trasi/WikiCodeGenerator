          </xs:element>
          <xs:element name="PayeeInfo" type="ct:Address" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the payee of the claim .</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um kröfueiganda.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Identifier" type="pt:Identifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">The unique identifier of the claimant that the claim relates to.</xs:documentation>
              <xs:documentation xml:lang="is">Auðkenni kröfuhafa sem innheimtukrafan tengist.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Reference" type="ct:Reference" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Refence determined by claimant. Alphanumeric, maximum 16 letters.</xs:documentation>
              <xs:documentation xml:lang="is">Tilvísunarnúmer ákveðið af kröfuhafa. Má vera tölu- og bókstafir, 16 stafa langt að hámarki.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FinalDueDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Final due date of claim.</xs:documentation>
              <xs:documentation xml:lang="is">Eindagi innheimtukröfu</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BillNumber" type="ct:BillNumber" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Billnumber, optional. Information provided by claimant.</xs:documentation>
              <xs:documentation xml:lang="is">Seðilnúmer, valkvætt. Upplýsingar settar af kröfuhafa.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CustomerNumber" type="pt:CustomerNumber" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Customer number, optional key used by claimant to identify payor. Necessary if payor is to be able to enable automotic debiting of claims.</xs:documentation>
              <xs:documentation xml:lang="is">
                Viðskiptanúmer greiðanda. Valkvæður lykill greiðanda kröfunar hjá kröfuhafa. Nauðsynlegur ef hægt á að vera að stofna beingreiðslusamning tengdan innheimtukröfu.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IsPartialPaymentAllowed" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Indicates whether partial payment is permitted.</xs:documentation>
              <xs:documentation xml:lang="is">Segir til um hvort greiða megi inn á innheimtukröfu eða ekki.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CategoryCodeName" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Name of claimant's text key, explanation of payment. Optional property of claim.</xs:documentation>
              <xs:documentation xml:lang="is">Heiti textalykill kröfuhafa, skýring á greiðslu. Valkvætt eigindi á innheimtukröfu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Print" type="pt:PrintInformation" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Indicates whether the payment slip needs to be printed or not. If the claimant has an agreement with the bank, it will handle the printing.</xs:documentation>
              <xs:documentation xml:lang="is">Segir til um hvort prenta þarf innheimtukröfu. Bankastofnun sér um útprentun ef um það er samkomulag.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="ClaimStatus">
    <xs:annotation>
      <xs:documentation xml:lang="en">Indicates the status of the claim.</xs:documentation>
      <xs:documentation xml:lang="is">Segir til um ástand innheimtukröfu.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Unpaid">
        <xs:annotation>
          <xs:documentation xml:lang="en">Claim has not yet been settled.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafa er ógreidd.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Paid">
        <xs:annotation>
          <xs:documentation xml:lang="en">Claim has been settled.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafa hefur greidd að fullu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Cancelled">
        <xs:annotation>
          <xs:documentation xml:lang="en">Claim has been cancelled.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafa hefur verið felld niður.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SecondaryCollection">
        <xs:annotation>
          <xs:documentation xml:lang="en">The claim has been sent to secondary collection.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafan hefur verið sett í milliinnheimtu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LegalCollection">
        <xs:annotation>
          <xs:documentation xml:lang="en">Legal action will be taken for dept recovery.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafa hefur verið sett í lögfræðiinnheimtu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Invalid">
        <xs:annotation>
          <xs:documentation xml:lang="en">Claim was invalid when created.</xs:documentation>
          <xs:documentation xml:lang="is">Innheimtukrafa hefur lent villu við stofnun.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ChangeWithdrawal">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Forbidden">
        <xs:annotation>
          <xs:documentation xml:lang="en">Both increase and decrease forbidden.</xs:documentation>
          <xs:documentation xml:lang="is">Bæði hækkun og lækkun bönnuð.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DecreaseAllowedIncreaseForbidden">
        <xs:annotation>
          <xs:documentation xml:lang="en">Decrease allowed, Increase forbidden.</xs:documentation>
          <xs:documentation xml:lang="is">Lækkun leyfð, Hækkun bönnuð.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Allowed">
        <xs:annotation>
          <xs:documentation xml:lang="en">Both increase and decrease allowed</xs:documentation>
          <xs:documentation xml:lang="is">Bæði hækkun og lækkun leyfð.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Communication">
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">Sendingarform á tilkynningu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IsPostalMail" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Postal mail</xs:documentation>
          <xs:documentation xml:lang="is-IS">Bréfpóstur</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReceiverAddress" type="ct:Address" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Ef isPostalMail er true þá er ath hvort eitthvað er í ReceiverAddress og það notað ef það valid annar er gert ráð fyrir að heimilifang viðtakanda sé sótt í þjóðskrá eftir kennitölu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Email" type="ct:Email" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Tölvupóstfang sem á að senda á.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SMS" type="ct:PhoneNumber"  minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Mobile phone number for SMS message (Short Message Service)</xs:documentation>
          <xs:documentation xml:lang="is-IS">Símanúmer sem á að senda smáskilaboð (SMS) í.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Language" type="xs:string" default="is-IS">
      <xs:annotation>
        <xs:documentation xml:lang="en-US"></xs:documentation>
        <xs:documentation xml:lang="is-IS">Tungumál sem tilkynningin á að vera á.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="Communications">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">List of communications.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Listi af tilkynningum.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Communication" type="pt:Communication" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="CustomerNumber">
    <xs:annotation>
      <xs:documentation xml:lang="en">Customer number of payor, claimant's unique code for the customer.</xs:documentation>
      <xs:documentation xml:lang="is">Einkvæmt viðskiptanúmer greiðanda í kerfi kröfuhafa.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="16"/>
      <xs:pattern value=".{0,16}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="DepositAccountInfo">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about deposit account.</xs:documentation>
      <xs:documentation xml:lang="is">Upplýsingar um innleggsreikning.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AgreementId" type="ct:AccountID">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment recipient's account.</xs:documentation>
          <xs:documentation xml:lang="is">Reikningsnúmer viðtakanda greiðslu. Einkvæmt númer reiknings sem er gefið út af viðskiptabanka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerInfo" type="pt:OwnerInfo">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information about payee.</xs:documentation>
          <xs:documentation xml:lang="is">Upplýsingar um greiðsluviðtakanda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CategoryCode" type="ct:CategoryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Category code.</xs:documentation>
          <xs:documentation xml:lang="is">Textalykill greiðslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Category" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Category code name.</xs:documentation>
          <xs:documentation xml:lang="is">Heiti textalykils.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DepositPayment">
    <xs:complexContent>
      <xs:extension base="pt:DepositPaymentAbstract">
        <xs:sequence>
          <xs:choice>
            <xs:element name="ABGiro" type="pt:ABGiro"/>
            <xs:element name="CGiro" type="pt:CGiro"/>
            <xs:element name="Claim" type="pt:Claim"/>
            <xs:element name="Transfer" type="pt:Transfer"/>
            <!--<xs:element name="Card" type="pt:Card"/>-->
            <!--<xs:element name="Mobile" type="pt:Mobile"/>-->
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DepositPaymentAbstract" abstract="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about the account that funds will be transferred to.</xs:documentation>
      <xs:documentation xml:lang="is">Upplýsingar um innlagnarreikning greiðslu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Amount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment amount in ISK. Please note that it is only possible to transfer money between accounts in ISK.</xs:documentation>
          <xs:documentation xml:lang="is">Upphæð greiðslu í krónum. Athugið að einungis er hægt að millifæra milli reikninga í íslenskum krónum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Receipt" type="pt:Communications" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Receipts that are sent when the payment is processed.</xs:documentation>
          <xs:documentation xml:lang="is">Kvittanir sem sendar eru þegar greiðsla er framkvæmd.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Description for the payment. This description is shown if the customer looks up the payment in the Internet bank.</xs:documentation>
          <xs:documentation xml:lang="is">Skýring greiðslu. Birtist í netbönkum ef að notandi flettir upp greiðslunni.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="BookingId" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The booking ID field can be used by the sender to include his own ID for the payment. This ID is also returned to the sender when the payment is queried (e.g. when the GetPaymentsResult operation is used).</xs:documentation>
          <xs:documentation xml:lang="is">Svæði sem notandi getur notað að eigin vild, t.d. með því að setja ID númer greiðslu í sínu kerfi. Þetta svæði fylgir greiðslum þegar gerð er fyrirspurn á greiðslur síðar meir, t.d. þegar verið er að sækja niðurstöðu framkvæmdar.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:int" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Payment number.</xs:documentation>
        <xs:documentation xml:lang="is">Greiðslunúmer.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="DepositPaymentError">
    <xs:sequence>
      <xs:element name="ErrorDecription" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">Error description.</xs:documentation>
          <xs:documentation xml:lang="is">Villulýsing.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:int" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="en">Payment number.</xs:documentation>
        <xs:documentation xml:lang="is">Greiðslunúmer.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="DepositPaymentInfo">
    <xs:complexContent>
      <xs:extension base="pt:DepositPaymentAbstract">
        <xs:sequence>
          <xs:choice>
            <xs:element name="ABGiroInfo" type="pt:ABGiroInfo"/>
            <xs:element name="CGiroInfo" type="pt:CGiroInfo"/>
            <xs:element name="ClaimInfo" type="pt:ClaimInfo"/>
            <xs:element name="TransferInfo" type="pt:TransferInfo"/>
            <!--<xs:element name="CardInfo" type="pt:CardInfo"/>
            <xs:element name="MobileInfo" type="pt:MobileInfo"/>-->
          </xs:choice>
          <xs:element name="DepositAccountInfo" type="pt:DepositAccountInfo"/>
          <xs:element name="Info" type="pt:DepositPaymentInfoType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the paytment's result.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um árangur greiðslunnar.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DepositPaymentInfoType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about the payment's status.</xs:documentation>
      <xs:documentation xml:lang="is">Upplýsingar um afdrif greiðslunnar.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TotalAmount" type="xs:decimal">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of the payment.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmountPaid" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount paid via the payment, including additional charges (cost, interest and discount).</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslunni, þar með talin viðbótargjöld (kostnaður, vextir og afsláttur).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Charges" type="pt:Charges">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information about additional charges pertaining to the payment.</xs:documentation>
          <xs:documentation xml:lang="is">Upplýsingar um viðbótargjöld vegna greiðslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="pt:DepositPaymentStatus" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Status of the payment.</xs:documentation>
          <xs:documentation xml:lang="is">Staða greiðslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reason" type="pt:ErrorReason">
        <xs:annotation>
          <xs:documentation xml:lang="en">Description of payment error.</xs:documentation>
          <xs:documentation xml:lang="is">Lýsing á greiðsluvillu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DepositPayments">
    <xs:annotation>
      <xs:documentation xml:lang="en">Collection of payments.</xs:documentation>
      <xs:documentation xml:lang="is">Safn greiðslna.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DepositPayment" type="pt:DepositPayment" minOccurs="0" maxOccurs="500">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment.</xs:documentation>
          <xs:documentation xml:lang="is">Greiðsla.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfPayments" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of payments.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi greiðslna.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="DepositPaymentStatus">
    <xs:restriction base="xs:string">
      <xs:enumeration value="UnPaid">
        <xs:annotation>
          <xs:documentation>0            Ógreitt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Changed">
        <xs:annotation>
          <xs:documentation>1            Breytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WithdrawalChanged">
        <xs:annotation>
          <xs:documentation>2            Úttektarupphæð breytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Paid">
        <xs:annotation>
          <xs:documentation>3            Greitt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Error">
        <xs:annotation>
          <xs:documentation>4            Villa</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Cancelled">
        <xs:annotation>
          <xs:documentation>5            Hætt við greiðslu, vegna villu í keyrslu á bunka</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Reversed">
        <xs:annotation>
          <xs:documentation>6            Bakfært</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Deleted">
        <xs:annotation>
          <xs:documentation>7            Eytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Booked">
        <xs:annotation>
          <xs:documentation>8            Bókuð</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BookedWithdrawalChanged">
        <xs:annotation>
          <xs:documentation>9            Bókuð, úttektarupphæð breytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DepositChanged">
        <xs:annotation>
          <xs:documentation>10          Innborgunarupphæð breytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BookedDepositChanged">
        <xs:annotation>
          <xs:documentation>11          Bókuð, innborgunarupphæð breytt</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Sent">
        <xs:annotation>
          <xs:documentation>20          Send til úttektar </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CardWithdrawalClosed">
        <xs:annotation>
          <xs:documentation>21          Úttekt af korti lokið</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ErrorReason">
    <xs:annotation>
      <xs:documentation xml:lang="en">Description of payment error.</xs:documentation>
      <xs:documentation xml:lang="is">Lýsing á greiðsluvillu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Text" maxOccurs="unbounded" type="pt:ErrorText">
        <xs:annotation>
          <xs:documentation xml:lang="en">Description of payment error.</xs:documentation>
          <xs:documentation xml:lang="is">Lýsing á greiðsluvillu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ErrorText" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ErrorText">
    <xs:annotation>
      <xs:documentation xml:lang="en">Description of payment error.</xs:documentation>
      <xs:documentation xml:lang="is">Lýsing á greiðsluvillu.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <!--<xs:attribute ref="xml:lang" use="required">-->
        <xs:attribute name="Language" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation xml:lang="en">Description language.</xs:documentation>
            <xs:documentation xml:lang="is">Tungumál lýsingar.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="Identifier">
    <xs:annotation>
      <xs:documentation xml:lang="en">An identifier unique to each claimant indicating primarily the account into which payments will be made. Can consist of alphanumeric characters, maximum three letters.</xs:documentation>
      <xs:documentation xml:lang="is">Auðkenni kröfuhafa sem vísar einkum á númer þess reiknings sem greiðsla til kröfuhafa á að ráðstafast inná. Má vera tölu- og bókstafir alls 3 stafa langt.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9A-Za-z]{3}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Mobile">
    <xs:annotation>
      <xs:documentation xml:lang="en">Describes an Mobile refill.</xs:documentation>
      <xs:documentation xml:lang="is">Mobile greiðslutegund er notað til þess að hlaða síma.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PhoneCompanyId" type="pt:PhoneCompanyId" minOccurs="0">
