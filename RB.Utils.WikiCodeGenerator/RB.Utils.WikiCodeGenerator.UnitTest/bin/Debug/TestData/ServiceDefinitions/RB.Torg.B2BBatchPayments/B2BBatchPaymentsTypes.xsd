              <xs:documentation xml:lang="en">Notice charge due for payment.</xs:documentation>
              <xs:documentation xml:lang="is">Upphæð tilkynningargjalds til greiðslu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DefaultChargeAmount" type="ct:Amount" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Default charge due for payment.</xs:documentation>
              <xs:documentation xml:lang="is">Upphæð vanskilagjalda til greiðslu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherCostsAmount" type="ct:Amount" minOccurs="0">
