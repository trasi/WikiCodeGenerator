  </xs:complexType>
  <xs:complexType name="BatchStatisticsAndCharges">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about different number and amounts of payments in the batch, interests, discounts and costs.</xs:documentation>
      <xs:documentation xml:lang="is">Upplýsingar um fjölda og upphæðir ólíkra greiðslubeiðna bunkans, vexti, afslætti og kostnað.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TotalAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of payments batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslna bunkans.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalPayments" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en">Numer of payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarfjöldi greiðslna í bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmountPaid" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount paid via the batch, including additional charges (cost, interest and discount).</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greidd með bunkanum, þar með viðbótargjöld (kostnaður, vextir og afsláttur).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Charges" type="pt:Charges">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information about charges pertaining to the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Upplýsingar um gjöld vegna bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchStatistics" type="pt:BatchStatistics">
        <xs:annotation>
          <xs:documentation xml:lang="en">Statistical information about the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Tölfræðiupplýsingar um bunkann.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="BatchStatus">
    <xs:annotation>
      <xs:documentation xml:lang="en">Status of a batch of payments</xs:documentation>
      <xs:documentation xml:lang="is">Mismunandi stöður sem greiðslubunki getur tekið</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="InProgress">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment batch is being processed</xs:documentation>
          <xs:documentation xml:lang="is">Greiðslubunki er í vinnslu hjá banka.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Completed">
        <xs:annotation>
          <xs:documentation xml:lang="en">The payment batch has been processed</xs:documentation>
          <xs:documentation xml:lang="is">Afgreiðslu á greiðslubunka lokið</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CompletedWithErrors">
        <xs:annotation>
          <xs:documentation xml:lang="en">The payment batch has been processed, but one or more payment has an error</xs:documentation>
          <xs:documentation xml:lang="is">Greiðslubunki hefur verið afgreiddur, en ein eða fleiri greiðslur hafa lent á villu</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Cancelled">
        <xs:annotation>
          <xs:documentation xml:lang="en">The payment batch has been cancelled by the user.</xs:documentation>
          <xs:documentation xml:lang="is">Þegar bunki hefur farið í stöðuna óstaðfestur og notandi hættir við að staðfesta hann t.d. í viðmóti netbanka fær bunkinn þessa stöðu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OnHold">
        <xs:annotation>
          <xs:documentation xml:lang="en">Forward payments waiting to be executed.</xs:documentation>
          <xs:documentation xml:lang="is">Greiðslubunki bíður þess að vera framkvæmdur þar sem valið hefur verið að framkvæma hann fram í tímanum</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NotConfirmed">
        <xs:annotation>
          <xs:documentation xml:lang="en">Unconfirmed payment batch. The payment batch needs to be confirmed by other means, e.g. in the internet bank.</xs:documentation>
          <xs:documentation xml:lang="is">Óstaðfestur bunki, bíður eftir að verða staðfestur handvirkt í t.d. netbanka.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BookingType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Offline">
        <xs:annotation>
          <xs:documentation xml:lang="en">Booking made offline.</xs:documentation>
          <xs:documentation xml:lang="is">Bókun ekki gerð á beinlínu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Online">
        <xs:annotation>
          <xs:documentation xml:lang="en">Booking made online.</xs:documentation>
          <xs:documentation xml:lang="is">Bókun gerð á beinlínu.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Card">
        <xs:annotation>
          <xs:documentation xml:lang="en">Booking made with card.</xs:documentation>
          <xs:documentation xml:lang="is">Bókun gerð á með korti.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
