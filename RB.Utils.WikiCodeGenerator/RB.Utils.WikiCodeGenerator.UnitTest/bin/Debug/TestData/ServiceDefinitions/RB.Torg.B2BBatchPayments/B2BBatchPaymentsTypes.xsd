        </xs:annotation>
      </xs:element>
      <xs:element name="TransferAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of transfers in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð millifærslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CardCount" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of card payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi kortagreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CardAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of card payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð kortagreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ErrorCount" type="xs:int" minOccurs="0">
