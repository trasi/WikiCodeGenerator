        </xs:annotation>
      </xs:element>
      <xs:element name="PaidAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of payments the batch which have been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslna í bunkanum sem hafa verið greiddar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaidCost" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of the batch's claim cost which has been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð kröfukostnaðar bunkans sem hefur verið greitt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UnpaidCount" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Numer of payments which have not been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi greiðslna sem hafa ekki verið greiddar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UnpaidAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of payments the batch which have not been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslna í bunkanum sem hafa ekki verið greiddar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UnpaidCost" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of the batch's claim cost which has not been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð kröfukostnaðar bunkans sem hefur ekki verið greitt.</xs:documentation>
        </xs:annotation>
      </xs:element>
