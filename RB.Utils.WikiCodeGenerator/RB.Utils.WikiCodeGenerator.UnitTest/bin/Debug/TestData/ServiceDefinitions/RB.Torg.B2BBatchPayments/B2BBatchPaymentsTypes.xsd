<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:pt="http://rb.b2b.is/schema/batchpayments/2012/06/01"
           xmlns:ct="http://rb.b2b.is/schema/commontypes/2012/06/01"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://rb.b2b.is/schema/batchpayments/2012/06/01"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           id="CommonTypes">
  <xs:import namespace="http://rb.b2b.is/schema/commontypes/2012/06/01" schemaLocation="B2BTypes.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"/>

  <!-- viðföng get-aðgerða rb.torg.b2bbatchpayments (queries) ... -->
  <xs:complexType name="GetBatchQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query used to retrieve a single batch.</xs:documentation>
      <xs:documentation xml:lang="is">Leitarskilrði sem er notað til þess að sækja stakan bunka.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BatchId" type="ct:OperationID">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch ID.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkanúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GetBatchesQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query used to search for batches.</xs:documentation>
      <xs:documentation xml:lang="is">Leitarskilrði sem er notað til þess að leta að greiðslubunkum.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BatchId" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch Number.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkanúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NameOfBatch" type="pt:NameOfBatch" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch name or part of it.</xs:documentation>
          <xs:documentation xml:lang="is">Hluti eða allt nafn bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FromCreatedDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Start date of a range of creation dates.</xs:documentation>
          <xs:documentation xml:lang="is">Upphafsdagsetning leitar fyrir skráningardag.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ToCreatedDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">End date of a range of creation dates.</xs:documentation>
          <xs:documentation xml:lang="is">Lokadagsetning leitar fyrir skrániningardag.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="pt:Status" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch's statuses (more than one status may be specified).</xs:documentation>
          <xs:documentation xml:lang="is">Segir til um hvaða staða eða stöður á að skila (fleiri en eitt gildi leyft).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Type" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch processing types (more than one type may be specified).</xs:documentation>
          <xs:documentation xml:lang="is">Vinnslutegundir í svari (fleiri en ein tegund leyfð).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BookingType" type="pt:BookingType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch booking type (only one booking type may be specified).</xs:documentation>
          <xs:documentation xml:lang="is">Bókunartegund bunka (bara ein bókunartegund leyfð).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WithdrawalChanged" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch withdrawal-change statuses (more than one status may be specified).</xs:documentation>
          <xs:documentation xml:lang="is">Úttektarbreytingarstöður bunka (fleiri en eitt gildi leyft).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FromValueDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Start date of a range of value dates.</xs:documentation>
          <xs:documentation xml:lang="is">Upphafsdagsetning leitar fyrir gildisdag</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ToValueDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">End date of a range of value dates.</xs:documentation>
          <xs:documentation xml:lang="is">Lokadagsetning leitar fyrir gildisdag</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CreatedBy" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch creator's User ID.</xs:documentation>
          <xs:documentation xml:lang="is">Notendanafn bunkaskapara.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Bank" type="ct:Bank" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch's bank.</xs:documentation>
          <xs:documentation xml:lang="is">Banki bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GetBatchStatisticsQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query used to retrieve a statistical information about a batch.</xs:documentation>
      <xs:documentation xml:lang="is">Leitarskilrði sem er notað til þess að fá tölfræði upplýsingar bunka.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BatchId" type="ct:OperationID">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch ID.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkanúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GetPaymentQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en">Query used to retrieve a single payment.</xs:documentation>
      <xs:documentation xml:lang="is">Leitarskilrði sem er notað til þess að sækja stakan greiðslu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BatchId" type="ct:OperationID">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batch ID.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkanúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentId" type="ct:OperationID">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment ID.</xs:documentation>
          <xs:documentation xml:lang="is">Greiðslunúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- tög vegna bunka og (bunka)greiðslna (í stafrófsröð, yður til hægðarauka) -->
  <xs:complexType name="ABGiro">
    <xs:annotation>
      <xs:documentation xml:lang="en">Describes an ABGiro payment.</xs:documentation>
      <xs:documentation xml:lang="is">ABGiro greiðslutegund er notað til þess að greiða AB-gíró seðla.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Account" type="ct:AccountID">
        <xs:annotation>
          <xs:documentation xml:lang="en">The ID of the account.</xs:documentation>
          <xs:documentation xml:lang="is">Reikningsnúmer viðtakanda greiðslu. Einkvæmt númer reiknings sem er gefið út af viðskiptabanka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reference" type="ct:Reference">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reference for the transaction. Usually the person ID of the originator is set in this field so the receiver of the payment can see from who the transaction is.</xs:documentation>
          <xs:documentation xml:lang="is">Tilvísunarnúmer, einkennir færsluna og kemur fram á reikningsyfirliti móttakanda.  Vænlegast er að setja kennitölu þess sem er að millifæra í þetta svæði þannig að móttakandi sjái hver er að greiða á sínu reikningsyfirliti.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BillNumber" type="ct:BillNumber">
        <xs:annotation>
          <xs:documentation xml:lang="en">Bill number for the payment.</xs:documentation>
          <xs:documentation xml:lang="is">Seðilnúmer. Sjö stafa tilvísunarnúmer sem birtist viðtakanda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CategoryCode" type="ct:CategoryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Category code for the payment.</xs:documentation>
          <xs:documentation xml:lang="is">Textalykill. Skilgreining á tegund færslu. Textalykill er skilgreindur í viðskiptabanka.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ABGiroInfo">
    <xs:complexContent>
      <xs:extension base="pt:ABGiro">
        <xs:sequence>
          <xs:element name="PayeeInfo" type="ct:Address">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the recipient of the AB-Giro payment.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um viðtakanda greiðslu AB-Gíróseðils.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CategoryCodeDescription" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Category code description</xs:documentation>
              <xs:documentation xml:lang="is">Lýsing á textalykli</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AddPaymentsResult">
    <xs:sequence>
      <xs:element name="DepositPaymentError" type="pt:DepositPaymentError" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en">Payment error.</xs:documentation>
          <xs:documentation xml:lang="is">Greiðsluvilla.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfErrors" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of errors.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi villna.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Batch">
    <xs:annotation>
      <xs:documentation xml:lang="en">Payment batch.</xs:documentation>
      <xs:documentation xml:lang="is">Greiðslubunki.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="pt:BatchAbstract">
        <xs:sequence>
          <xs:element name="WithdrawalAccount" type="pt:WithdrawalAccount" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the account from which funds will be drawn.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um úttektarreikning.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Notification" type="pt:Communications" minOccurs="0"/>
          <xs:element name="Payments" type="pt:DepositPayments" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the accounts to which funds will be transferred.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um innlagnarreikninga greiðslu.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BatchAbstract" abstract="true">
    <xs:sequence>
      <xs:element name="Name" type="pt:NameOfBatch" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Name of the batch to create. If not specified the name will be generated.</xs:documentation>
          <xs:documentation xml:lang="is">Nafn bunka sem á að stofna. Ef nafn er ekki sett þá er búið til nafn.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CreatedDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Creation date and time of the batch. If not specified the current date and time will be used.</xs:documentation>
          <xs:documentation xml:lang="is">Stofndagsetning og -tími bunka. Ef ekki tiltekið er núverandi dagur og tími notaður.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="pt:Status" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Status of the batch. If not specified a default status will be assigned.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkastaða. Ef staða er ekki tiltekin mun sjálfgefin staða vera notuð.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchType" type="pt:Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of the batch. If not specified a default type will be assigned.</xs:documentation>
          <xs:documentation xml:lang="is">Bunkategund. Ef tegund er ekki tiltekin mun sjálfgefin tegund vera notuð.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BookingType" type="pt:BookingType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Boking type of the batch. If not specified a default booking type will be assigned.</xs:documentation>
          <xs:documentation xml:lang="is">Bókunrtegund bunka. Ef bókunartegund er ekki tiltekin mun sjálfgefin tegund vera notuð.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--<xs:element name="WithdrawalChanged" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Has the batch's amount been changed?</xs:documentation>
          <xs:documentation xml:lang="is">Hefur upphæð bunkans verið breytt?</xs:documentation>
        </xs:annotation>
      </xs:element>-->
      <xs:element name="ChangeWithdrawal" type="pt:ChangeWithdrawal" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The batch's withdrawal change permission.</xs:documentation>
          <xs:documentation xml:lang="is">Leyfi til breytingar á upphæð bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValueDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The batch's value date.</xs:documentation>
          <xs:documentation xml:lang="is">Greiðsludagur bunkans.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SystemInfo" type="pt:SystemInfo" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:int" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Batch number.</xs:documentation>
        <xs:documentation xml:lang="is">Bunkanúmer.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <!-- o.s.frv. -->
  </xs:complexType>
  <xs:complexType name="Batches">
    <xs:annotation>
      <xs:documentation xml:lang="en">A collection of batches which meet some search criteria.</xs:documentation>
      <xs:documentation xml:lang="is">Safn bunka sem uppfylla tiltekin leitarskilyrði.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Batch" type="pt:BatchInfo" minOccurs="0" maxOccurs="500">
        <xs:annotation>
          <xs:documentation xml:lang="en">Batches.</xs:documentation>
          <xs:documentation xml:lang="is">Buknar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfBatches" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of batches.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <!-- o.s.frv. -->
  </xs:complexType>
  <xs:complexType name="BatchInfo">
    <xs:complexContent>
      <xs:extension base="pt:BatchAbstract">
        <xs:sequence>
          <xs:element name="Info" type="pt:BatchInfoType">
            <xs:annotation>
              <xs:documentation xml:lang="en">Additional information about the batch.</xs:documentation>
              <xs:documentation xml:lang="is">Aukaupplýsingar um bunkann.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="WithdrawalAccountInfo" type="pt:WithdrawalAccountInfo">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the account from which funds will be drawn.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um úttektarreikning.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Payments" type="pt:DepositPaymentInfo" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="en">Information about the account to which funds will be transferred.</xs:documentation>
              <xs:documentation xml:lang="is">Upplýsingar um innlagnarreikning greiðslu.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BatchInfoResult">
    <xs:sequence>
      <xs:element name="TotalAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of payments batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greiðslna bunkans.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalPayments" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en">Numer of payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarfjöldi greiðslna í bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmountPaid" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount paid via the batch, including additional charges (cost, interest and discount).</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð greidd með bunkanum, þar með talin viðbótargjöld (kostnaður, vextir og afsláttur).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalPaidCosts" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Costs pertaining to the batch once it has been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Kostnaður vegna bunka er hann hefur verið greiddur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EstimatedTotalAmountPaid" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Estimated total amount paid via the batch, including additional charges (cost, interest and discount).</xs:documentation>
          <xs:documentation xml:lang="is">Áætluð heildarupphæð greidd með bunkanum, þar með talin viðbótargjöld (kostnaður, vextir og afsláttur).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EstimatedTotalPaidCosts" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en">Estimated costs pertaining to the batch when it will have been paid.</xs:documentation>
          <xs:documentation xml:lang="is">Áætlaður kostnaður vegna bunka þegar hann mun hafa verið greiddur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Charges" type="pt:Charges">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information about charges pertaining to the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Upplýsingar um gjöld vegna bunka.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchStatistics" type="pt:BatchStatistics">
        <xs:annotation>
          <xs:documentation xml:lang="en">Statistical information about the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Tölfræðiupplýsingar um bunkann.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Currency" type="ct:CurrencyCode">
        <xs:annotation>
          <xs:documentation xml:lang="en">Currency code of the batch (determined by the currency of the withdrawal account).</xs:documentation>
          <xs:documentation xml:lang="is">Gjaldmiðill bunkans (ákvarðaður af gjaldmiðli úttektarreiknings).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BatchInfoType">
    <xs:choice>
      <xs:element name="Error" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">Description of batch error.</xs:documentation>
          <xs:documentation xml:lang="is">Lýsing á bunkavillu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Result" type="pt:BatchInfoResult">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information about the batch payment result.</xs:documentation>
          <xs:documentation xml:lang="is">Upplýsingar um árangur bunkagreiðslunnar.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="BatchNotification">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Batch notifications.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Tilkynningar vegna bunka.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Recipient" type="pt:Communications">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">List of communications.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Listi af tilkynningum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Notify" type="pt:Notification" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Under which conditions should notifications be sent.</xs:documentation>
          <xs:documentation xml:lang="is">Undir hvaða kringumstæðum skal senda tilkynningar.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BatchStatistics">
    <xs:sequence>
      <xs:element name="ABGiroCount" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of AB-Giro payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi AB-Gírógreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ABGiroAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of AB-Giro payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð AB-Gírógreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CGiroCount" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of C-Giro payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi C-Gírógreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CGiroAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of C-Giro payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð C-Gírógreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClaimCount" type="xs:int" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of claim payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Fjöldi kröfugreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClaimAmount" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of claim payments in the batch.</xs:documentation>
          <xs:documentation xml:lang="is">Heildarupphæð kröfugreiðslna í bunkanum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClaimCost" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Total amount of claim cost in the batch.</xs:documentation>
