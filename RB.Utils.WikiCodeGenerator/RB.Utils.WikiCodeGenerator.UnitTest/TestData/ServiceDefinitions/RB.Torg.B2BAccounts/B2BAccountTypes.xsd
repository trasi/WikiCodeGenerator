<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://rb.b2b.is/schema/accounts/2012/06/01" xmlns:at="http://rb.b2b.is/schema/accounts/2012/06/01" xmlns:ct="http://rb.b2b.is/schema/commontypes/2012/06/01" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" id="CommonTypes">
  <xs:import namespace="http://rb.b2b.is/schema/commontypes/2012/06/01" schemaLocation="B2BTypes.xsd"/>

<!--GetAccountStatementQuery-->
  
  <xs:complexType name="GetAccountStatementQuery">
    <xs:sequence>
      <xs:element name="AgreementId" type="ct:AccountID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The account information is requested for.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Reikningsnúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateFrom" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">From date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dagsetning frá og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateTo" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">To date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dagsetning til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AmountFrom" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Amount from.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð frá.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AmountTo" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Amount to.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð til.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SearchText" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Search text.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Leitar strengur</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StatementType" type="at:StatementType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Hvat kind of transactions.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Hvað á að skila, öllum færslum, bara innborgunum, bara útborgunum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Paging" type="at:Paging">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Used to limit the records in the response to a certain range.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Notað til að takmarka fjölda færslna í svari.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Ordering" type="at:Ordering">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Dálkurinn sem á að raða eftir.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Paging" >
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">Upplýsingar fyrir paging.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Page" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Blaðsíðan sem á að birta.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PageSize" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Fjöldi færslna á hverri blaðsíðu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Ordering">
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">Listi af svæðum sem á að raða eftir.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Order" type="at:OrderBy" maxOccurs="100"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OrderBy" >
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">Dálkurinn sem á að raða eftir.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
    <xs:element name="ColumnName" type="xs:string">
      <xs:annotation>
        <xs:documentation xml:lang="en-US"></xs:documentation>
        <xs:documentation xml:lang="is-IS">Heiti svæðis sem á að raða eftir.</xs:documentation>
      </xs:annotation>
    </xs:element>
    </xs:sequence>
    <xs:attribute name="Direction" type="at:SortDirection" default="Desc">
      <xs:annotation>
        <xs:documentation xml:lang="en-US"></xs:documentation>
        <xs:documentation xml:lang="is-IS">Attribute sem segir til um hvort þetta er hækkkandi eða lækkandi röð.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:simpleType name="SortDirection">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Asc" >
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Ascending Order. Arranged from smallest to largest.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Hækkandi röð.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Desc">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Desending Order. Arranged from largest to smallest.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Lækkandi röð.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StatementType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="All">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">All transaction.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Allar færslur, bæði innborganir og útborganir, hvaðan sem þær koma.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Deposits">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Deposits.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Innborganir.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Withdrawals">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Withdrawals.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Útborganir.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Debetcard">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Transaction from the debetcards.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greiðslur með debetkorti.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Internetbank">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Payments done in the internetbank.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greiðlur gerði í netbankanum.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!--GetAccountStatementResult-->

  <xs:complexType name="GetAccountStatementResult">
    <xs:sequence>
      <xs:element name="AccountStatement" type="at:AccountStatement" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Upplysingar um reikning ásamt færslum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PagingInfo" type="at:PagingInfo"  minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Samtals færslur fyrir þetta leitarskilyrði, notað til að teikna upp page-ing.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AccountStatement">
    <xs:sequence>
      <xs:element name="AgreementId" type="ct:AccountID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The ID of the account.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Reikningur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Currency" type="ct:CurrencyCode">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The currency of the account.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Mynt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OverdraftAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Overdraft permission.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Yfirdráttarheimild.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BalanceAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The balance of the account.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Staða reiknings.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AvailableAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The amount available to the account holder.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Til ráðstöfunar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="ct:Status">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The status of the account (open/closed).</xs:documentation>
          <xs:documentation xml:lang="is-IS">Ástand reiknings.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmountWaiting" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The total amount of transactions waiting that exceeded amount available.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Heildarupphæð innistæðulausra úttekta.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IBAN" type="ct:IBAN">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">IBAN Number.</xs:documentation>
          <xs:documentation xml:lang="is-IS">IBAN númer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomAccountName" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Name set by account owner.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Nafn sett af reikningseiganda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FinancialEvents" type="at:FinancialEvents" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Account transaction list.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Reikningsyfirlitið sjálft.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FinancialEvents" >
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">List af financials events færslum.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FinancialEvents" type="at:FinancialEvent" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS"></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FinancialEvent">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Account Transaction.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Upplýsingar um staka Financials event færslu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="EventId" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Uniquely identifies this transaction with the originating system.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Einkennisnúmer færslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EventTimeStamp" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The date when this transaction was booked.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Bókunardagur, hreyfingardagur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValueDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Value Date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Vaxtadags.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchNumber" type="ct:BatchNumber">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Batch Identifier.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Bunkanúmer. RB bunkanúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RedeemingBank" type="ct:Bank">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The bank where the transaction originated.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Innlausnarbanki.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionCode" type="ct:Transaction">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Transaction Key.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Færslulykill.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Færslunúmer (FANR).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reference" type="ct:Reference" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Tilvísun.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReferenceLong" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Löng tilvísun.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BillNumber" type="ct:BillNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Bill number.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Seðilinúmer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CategoryCode" type="ct:CategoryCode">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Category code</xs:documentation>
          <xs:documentation xml:lang="is-IS">Textalykill.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Category" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Expands on the category code and gives the fulltext explanation.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skýring textalykils.  Lengri skýring textalykils sem til er í kerfunum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReferenceDescription" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Expands on the information in the reference, usually by trying to matching the information entered with person identification numbers.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skýring tilvísunar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayorId" type="ct:PersonID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Person ID of payor.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala greiðanda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExternalId" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Ytra id sem vv getur hengt á færsluna.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Amount.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BalanceAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The balance of the account after this transaction.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Staða eftir færslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CurrencyId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Mynt reiknings.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AmountLcy" type="ct:Amount" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð í ISK</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DueDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Gjalddagi.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BenefactorId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala móttakanda(KTGR eða SUBSTR(TILV,1,10) í AH).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EvnetGroupId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">MillifærsluId, Id á móthreyfingu sem myndar Inn og út.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PagingInfo" >
    <xs:annotation>
      <xs:documentation xml:lang="en-US"></xs:documentation>
      <xs:documentation xml:lang="is-IS">upplýsingar um paging út frá því sem var sent inn.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Page" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Blaðsíðan sem á að birta.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PageSize" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Fjöldi færslna á hverri blaðsíðu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalRows" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS"> Heildarfjöldi færslna í fyrirspurninni.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalPages" type="xs:int">
        <xs:annotation>
          <xs:documentation xml:lang="en-US"></xs:documentation>
          <xs:documentation xml:lang="is-IS">Heildafjöldi blaðsíðna í fyrirspurninni.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>



